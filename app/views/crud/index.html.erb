<table border="1">
<thead>
   <th>Title</th>
   <th>Description</th>
   <th>Created_date</th>
   <th>Actions</th>
 </thead>

 <%= @albums.each do |album| %>
 <tr>
    <td>
     <%= album.title %> 
    </td>
    <td>
     <%= album.description %> 
   </td>
    <td>
     <%= album.created_date %> 
  </td>
 </tr>
 <% end %>

 </table>